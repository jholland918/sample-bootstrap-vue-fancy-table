      "sourcesContent": [
        "<template>\r\n  <div class=\"page-size-select\">\r\n    Show\r\n    <b-form-select v-model=\"selected\" :options=\"options\"></b-form-select>\r\n    entries\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"page-size-select\",\r\n  data() {\r\n    return {\r\n      selected: 10,\r\n      options: [\r\n        { value: 10, text: \"10\" },\r\n        { value: 25, text: \"25\" },\r\n        { value: 50, text: \"50\" },\r\n        { value: 100, text: \"100\" },\r\n      ],\r\n    };\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.page-size-select {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.page-size-select > select {\r\n  margin-left: 1rem;\r\n  margin-right: 1rem;\r\n  width: 5rem;\r\n}\r\n</style>"
      ]
    },
